{{>navigationBarTech}}

<div class="mainContent">
    <h1>Reservations</h1>
    {{>filter}}
    <table id="reservations">
        <tr>
            <th>Computer Lab</th>
            <th>Date</th>
            <th>Time Slot</th>
            <th>Email</th>
            <th>User</th>
            <th>Seat Number</th>
            <th></th>
        </tr>
        {{#each all_reserve_data}}
        <tr>
            <td>{{computer_lab}}</td>
            <td>{{date_reserved}}</td>
            <td>{{time_slot}}</td>
            <td>{{email}}</td>
            <td>{{user}}</td>
            <td>{{seat_num}}</td>
            <td>
                <div class="cont">
                    {{!-- change html --}}
                    <form action="/editReservationTech">
                        <button type="submit" class="reserveBtn">Edit</button>
                    </form>
                    <form action="/deleteReserveTech">
                        <button type="submit" class="reserveBtn">Delete</button>
                    </form>
                </div>
            </td>
        </tr>
        {{/each}}
    </table>
</div>