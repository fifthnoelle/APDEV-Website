<div class="reserveForm">
    <form action="">
        <div class="formPersonal">
            <div class="formQuestion">
                <label for="Name"><b>Name</b></label>
                <input type="text">
            </div>
        </div>
        <div class="formPersonal">
            <div class="formQuestion">
                <label for="email"><b>Email</b></label>
                <input type="text">
            </div>
        </div>
        <div class="formLab">
            <div class="formQuestion">
                <label for="laboratory"><b>Computer Laboratory</b></label>
                <select name="laboratory" id="lab">
                    <option value="--">-- Select Lab</option>
                    <option value="GK304A">GK304A</option>
                    <option value="GK503B">GK503B</option>
                </select>
            </div>
        </div>
        <div class="formDateTime">
            <div class="formQuestion">
                <label for="date"><b>Date</b></label>
                <input type="date">
            </div>
            <div class="formQuestion">
                <label for="time"><b>Time</b></label>
                <select name="time" id="time">
                    <option value="1">8:00 - 9:30</option>
                    <option value="2">10:00 - 11:30</option>
                    <option value="3">12:00 - 13:30</option>
                    <option value="4">14:00 - 15:30</option>
                    <option value="5">16:00 - 17:30</option>
                    <option value="6">18:00 - 19:30</option>
                </select>
            </div>
        </div>
        <input type="submit" class="submitBtn">
    </form>
</div>